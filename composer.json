{
    "name": "fakeeh/laravel-secure-email",
    "description": "Laravel package to monitor and handle AWS SES email complaints, bounces, and deliveries via SNS notifications - Laravel 12 compatible",
    "keywords": [
        "laravel",
        "laravel-12",
        "aws",
        "ses",
        "sns",
        "email",
        "bounce",
        "complaint",
        "monitoring"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fakeeh Care Group Development Team",
            "email": "webhosting@fakeeh.care"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/mail": "^12.0",
        "aws/aws-php-sns-message-validator": "^1.9"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Fakeeh\\SecureEmail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fakeeh\\SecureEmail\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fakeeh\\SecureEmail\\SesMonitorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
